{
    "name":"News Guard",
    "version":"1.0",
    "manifest_version":2,
    "browser_action":{
        "default_popup":"popup.html",
        "default_title":"News Guard"
    },
    "permissions":[
        "webRequest",
        "notifications",
        "tabs"
    ],
    "content_security_policy": "script-src 'self' https://www.gstatic.com/firebasejs/5.5.8/firebase.js https://cdn.firebase.com https://*.firebaseio.com; object-src 'self'"
}
